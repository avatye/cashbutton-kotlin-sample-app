<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical"
    tools:context=".main.CashButtonChannelingActivity">

    <Button
        android:id="@+id/channelingButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="50dp"
        android:text="캐시버튼 채널링 실행" />


    <Button
        android:id="@+id/GoToSetting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="진입점 노출 설정" />


    <Button
        android:id="@+id/launch_activity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="Launch Activity" />


    <Button
        android:id="@+id/launch_custom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Launch Custom" />

</LinearLayout>